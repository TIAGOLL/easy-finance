{
  "version": 2,
  "builds": [
    {
      "src": "/dist/main.js",
      "use": "@vercel/node",
      "config": {
        "nodeVersion": "18.x",
        "packageManager": "pnpm",
        "installCommand": "pnpm install --frozen-lockfile;",
        "buildCommand": "db:migrate:deploy; pnpm generate; pnpm build;",
        "outputDirectory": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "(.*)",
      "dest": "/dist/main.js"
    }
  ]
}