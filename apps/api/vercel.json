{
  "version": 2,
  "buildCommand": "pnpm db:migrate:deploy; pnpm generate; pnpm build",
  "outputDirectory": "dist",
  "installCommand": "pnpm i --no-frozen-lockfile;",
  "builds": [
    {
      "src": "/apps/api/src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "methods": [
        "GET",
        "POST",
        "PATCH",
        "PUT",
        "DELETE"
      ],
      "src": "/apps/api/src/main.ts"
    }
  ]
}