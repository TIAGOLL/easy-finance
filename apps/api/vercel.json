{
  "version": 2,
  "builds": [
    {
      "src": "/src/main.ts",
      "use": "@vercel/node",
      "config": {
        "packageManager": "pnpm",
        "installCommand": "pnpm install --frozen-lockfile; db:migrate:deploy; pnpm generate; ",
        "buildCommand": "pnpm build;",
        "outputDirectory": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "(.*)",
      "dest": "/src/main.ts"
    }
  ]
}