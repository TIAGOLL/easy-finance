{
  "version": 2,
  "builds": [
    {
      "src": "/apps/api/dist/src/main.js",
      "use": "@vercel/node"
    },
    {
      "src": "/apps/web/dist/index.html",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/apps/api/src/main.ts" },
    { "src": "/web/(.*)", "dest": "/apps/web/src/main.tsx" }
  ]
}
