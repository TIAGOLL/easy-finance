{
  "name": "@easyfinance/api-types",
  "version": "0.0.0",
  "private": true,
  "scripts": {},
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*"
      ]
    }
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./*": {
      "import": "./dist/*.js",
      "require": "./dist/*.js"
    }
  },
  "dependencies": {
    "@nestjs/mapped-types": "*"
  },
  "devDependencies": {
    "@easyfinance/eslint-config": "workspace:*",
    "@easyfinance/typescript-config": "workspace:*",
    "@easyfinance/prettier-config": "workspace:*",
    "@types/node": "^20.3.1",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "typescript": "5.5.4"
  },
  "prettier": "@easyfinance/prettier-config",
  "eslintConfig": {
    "extends": [
      "@easyfinance/eslint-config/nest"
    ]
  }
}